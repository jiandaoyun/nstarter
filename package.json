{
  "name": "nstarter",
  "description": "Starter tool for node projects with template.",
  "version": "0.4.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiandaoyun/nstarter"
  },
  "license": "MIT",
  "bin": {
    "nstarter": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.json",
    "prestart": "tsc --build tsconfig.json",
    "start": "node ./dist/index.js",
    "eslint": "eslint --ext .js,.ts --config .eslintrc.js ./src",
    "eslint:html": "npm run eslint -- --output-file ./lint/eslint.html --format html"
  },
  "keywords": [
    "project starter",
    "node.js",
    "typescript",
    "nstarter",
    "starter",
    "template"
  ],
  "dependencies": {
    "async": "^3.2.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "inquirer": "^6.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.31",
    "shelljs": "^0.8.2",
    "simple-git": "^1.107.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.7.0",
    "winston-transport": "^4.3.0",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/fs-extra": "^5.0.5",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "0.0.43",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.164",
    "@types/node": "^12.12.6",
    "@types/yargs": "^12.0.9",
    "eslint": "6.8.0",
    "eslint-config-nstarter": "1.0.6",
    "nstarter-tsconfig": "0.2.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0"
  }
}
