{
  "name": "nstarter",
  "description": "Startup CLI tool for node projects with template.",
  "version": "0.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiandaoyun/nstarter"
  },
  "license": "MIT",
  "bin": {
    "nstarter": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.json",
    "prestart": "tsc --build tsconfig.json",
    "start": "node ./dist/index.js",
    "eslint": "eslint --ext .js,.ts --config .eslintrc.js ./src",
    "eslint:html": "npm run eslint -- --output-file ./lint/eslint.html --format html"
  },
  "keywords": [
    "project starter",
    "node.js",
    "typescript",
    "nstarter",
    "starter",
    "template"
  ],
  "dependencies": {
    "async": "^3.2.2",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "inquirer": "^6.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.33",
    "simple-git": "^3.11.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^3.7.0",
    "winston-transport": "^4.3.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.10",
    "@types/fs-extra": "^5.0.5",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "0.0.43",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.164",
    "@types/node": "14.14.31",
    "@types/yargs": "^17.0.11",
    "eslint": "^7.32.0",
    "eslint-config-nstarter": "^2.1.2",
    "nstarter-tsconfig": "^0.3.0",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0"
  }
}
